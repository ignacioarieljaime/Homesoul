<%- include('../layout/header') %>
<div class="row">
  <div class="col-lg-12 col-12">
    <div class="card card-info card-outline">
      <div class="card-header">
        <h3 class="card-title">List of Records</h3>
      </div>
      <div class="card-body">
        <div class="row">
          <div class="col-md-12">
            <table id="<%=tableId%>" data-loadurl="<%= loadList %>" class="table table-bordered table-striped dataTable">
              <thead>
                <tr>
                  <th data-name="name" data-data="name">Name</th>
                  <th data-name="email" data-data="email">Email</th>
                  <th data-name="phone" data-data="phone">Phone</th>
                  <th data-name="city" data-data="city">City</th>
                  <th data-name="province" data-data="province">Province</th>
                  <th data-name="country" data-data="country">Country</th>
                  <th data-name="status" data-data="status">Status</th>
                  <th data-name="action" data-data="action" data-sortable="false">Action</th>
                </tr>
              </thead>
              <tbody>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%- include('../layout/footer') %>
<script>
  var crudTable = null;
  jQuery(document).ready(function(e) {
    crudTable = siteServerSideDatatable('#<%= tableId %>', {});
  });
</script>