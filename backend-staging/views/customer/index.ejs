<%- include('../layout/header') %>
<div class="row">
  <div class="col-lg-12 col-12">
    <div class="card card-info card-outline">
      <div class="card-header">
        <h3 class="card-title">List of Records</h3>
      </div>
      <div class="card-body">
        <div class="row">
          <div class="col-md-12">
            <table id="<%=tableId%>" data-loadurl="<%= loadList %>" class="table table-bordered table-striped dataTable">
              <thead>
                <tr>
                  <th data-name="firstName" data-data="firstName">First Name</th>
                  <th data-name="lastName" data-data="lastName">Last Name</th>
                  <th data-name="emailID" data-data="emailID">Email</th>
                  <th data-name="addressLine1" data-data="addressLine1">Address1</th>
                  <th data-name="addressLine2" data-data="addressLine2">Address2</th>
                  <th data-name="status" data-data="status">Status</th>
                </tr>
              </thead>
              <tbody>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%- include('../layout/footer') %>
<script>
  var crudTable = null;
  jQuery(document).ready(function(e) {
    crudTable = siteServerSideDatatable('#<%= tableId %>', {});
  });
</script>