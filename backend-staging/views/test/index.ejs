<div class="row">
    <div class="col-lg-12 col-12">
      <div class="card card-info card-outline">
        <div class="card-header">
          <h3 class="card-title">List of Records</h3>
          <div class="card-tools">
            <a href="/test/add"><button type="button" class="btn btn-info btn-sm ml-3 ">Add
                New</button></a>
          </div>
        </div>
        <div class="card-body">
          <div class="row">
            <div class="col-md-12">
              <table id="<%=tableId%>" data-loadurl="<%= loadList %>" class="table table-bordered table-striped dataTable">
                <thead>
                  <tr role="row">
                    <th data-name="title" data-data="title">Title</th>
                    <th data-name="desc" data-data="desc">Desc.</th>
                    <th data-name="action" data-data="action" data-sortable="false">Action</th>
                  </tr>
                </thead>
                <tbody>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>